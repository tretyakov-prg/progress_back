<!DOCTYPE html>
<html lang="en">
<head>
  <title>User List</title>
</head>
<body>
    <div class="table-data">
        <h2>User test List</h2>
        <table border="1">
            <tr>
                <th>S.N</th>
                <th>Name</th>
                <th>NicName</th>
                <th>true</th>
                <th>false</th>
                <th>Edit</th>
                <th>Delete</th>
            </tr>
            
            <%
            if(userData.length!=0){
            var i=1;
            userData.forEach(function(data){
            %>
            <tr>
                <td><%=i; %></td>
                <td><%=data.name %></td>
                <td><%=data.nicname %></td>
                <td><%=data.ansver_true %></td>
                <td><%=data.ansver_false %></td>
                <td><a href="/users/edit/<%=data.id%>">Edit</a></td>
                <td><a href="/users/delete/<%=data.id%>">Delete</a></td>
            </tr>
            <%  i++; }) %>
            <% } else{ %>
                <tr>
                    <td colspan="7">No Data Found</td>
                </tr>
            <% } %>
        </table>
    </div>
</body>
</html>